{
    "manifest_version": 2,
    "name": "Word Block extension",
    "version": "0.1.0",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png",
      "default_title": "Word Block"
  },
  "icons": {
    "128": "logo.png"
},
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ]
  }